    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/platform-ws": "^9.3.9",
    "@nestjs/websockets": "^9.2.1",
    "@paypal/payouts-sdk": "^1.1.1",
    "@prisma/client": "^4.10.1",
    "@types/argon2": "^0.15.0",
    "argon2": "^0.29.1",
    "class-transformer": "^0.5.1",
